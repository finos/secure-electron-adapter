{
  "name": "@chartiq/secure-electron-adapter",
  "productName": "secure-electron-adapter",
  "description": "Adapter for running a secured workspace on electron",
  "version": "3.12.1",
  "main": "./dist/index.js",
  "files": [
    "dist/",
    "exports.js",
    "deploy/configs"
  ],
  "scripts": {
    "lint:quiet": "eslint --quiet ./src",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "startbrk": "---need to re-write this---gulp dev:noLaunch --gulpfile=./finsemble-seed/gulpfile.js &  set ELECTRON_DEV=true && electron index.js --inspect-brk=5858  --remote-debugging-port=9090   --manifest http://localhost:3375/configs/openfin/manifest-local.json",
    "tslint": "tslint -c tslint.json src/**/*.ts",
    "tslint:fix": "tslint -c tslint.json src/**/*.ts --fix",
    "dev": "set NODE_ENV=development&& gulp buildE2O",
    "prod": "set NODE_ENV=production&& gulp buildE2O",
    "test": "npx electron-mocha src/**/*.spec.js --timeout 5000",
    "build:prod": "webpack --mode=production --config ./build/webpack.exports.js&& webpack --mode=production --config ./build/webpack.index.js&& webpack --mode=production --config ./build/webpack.renderer.js",
    "build": "webpack --env.prod --config ./build/webpack.exports.js&& webpack --env.prod --config ./build/webpack.index.js&& webpack --env.prod --config ./build/webpack.renderer.js"
  },
  "build": {
    "appId": "e2o"
  },
  "author": "ChartIQ",
  "license": "",
