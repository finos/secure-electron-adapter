    "electron-rebuild": "1.8.2",
    "electron-squirrel-startup": "1.0.0",
    "electron-winstaller": "3.0.4",
    "eslint": "5.9.0",
    "eslint-plugin-import": "2.14.0",
    "proxyquire": "2.1.1",
    "fast-safe-stringify": "2.0.6",
    "gulp": "4.0.0",
